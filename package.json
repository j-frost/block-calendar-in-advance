{
    "name": "block-calendar-in-advance",
    "version": "0.0.1",
    "description": "Block Google Calendar from inside of an AppsScript project",
    "main": "src/index.ts",
    "scripts": {
        "clean": "rimraf dist/",
        "lint": "eslint -c .eslintrc.js --ext .ts src/",
        "pretest": "npm run lint",
        "test": "mocha --require ts-node/register --extensions ts 'src/**/*.spec.ts'",
        "prebuild": "npm run test",
        "build": "webpack && cpx appsscript.json dist && cpx \"src/**/*.html\" dist",
        "build:clean": "npm run clean && npm run build",
        "predeploy": "npm run build:clean",
        "deploy": "clasp push"
    },
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/google-apps-script": "^1.0.45",
        "@types/mocha": "^9.1.0",
        "@types/sinon": "^10.0.8",
        "@types/sinon-chai": "^3.2.8",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "chai": "^4.3.6",
        "cpx": "^1.5.0",
        "eslint": "^8.7.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jsdoc": "^37.7.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.0.0",
        "gas-webpack-plugin": "^2.1.0",
        "mocha": "^9.2.0",
        "prettier": "^2.5.1",
        "sinon": "^12.0.1",
        "sinon-chai": "^3.7.0",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.2"
    },
    "author": "Julian Felix Rost <j-frost@a3re.net>",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "git@github.com:j-frost/block-calendar-in-advance.git",
        "directory": "."
    }
}
